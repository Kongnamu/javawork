package switchexample;

public class SwitchCaseTest3 {

	public static void main(String[] args) {
		// 사칙연산
		// 변수: num1, num2, operator(기호), result(결과)
		// ex) 2 * 3 = 6
		
		int num1 = 10;
		int num2 = 2;
		char operator = '+'; // 값을 초기화
		int result = 0;
		
		switch(operator) {
		case '+':
			result = num1 + num2;
		}
		System.out.println("결과 값은 " + result + "입니다.");

	}

}
