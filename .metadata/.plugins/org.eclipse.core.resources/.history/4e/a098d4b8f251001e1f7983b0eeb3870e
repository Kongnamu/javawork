package customers;

public class VIPcustomer extends Customer{
	private int agentId; // 전문 상담원
	double saleRatio;    // 구매 할인율
	
	public VIPcustomer() {
		customerGrade = "VIP";
		saleRatio = 0.1;   // 10%
		bonusRatio = 0.05; // 5%
	}
	
	public void setagentId(int agentId) {
		this.agentId = agentId;
	}

}
