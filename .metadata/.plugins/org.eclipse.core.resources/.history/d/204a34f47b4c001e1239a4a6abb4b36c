package ifexample;

public class MaxNumber {

	public static void main(String[] args) {
		// 두 수중 큰 값을 찾는 프로그램
		// 변수: n1, n2, max
		int n1 = 10;
		int n2 = 20;
		int max;
		
		// 연산
		if(n1 >= n2) {
			max = n1;
		}else { //n1 < n2
            max = n2;
	    }
		
		// 출력
		System.out.println("두 수중 큰 값은 " + max + "입니다.");

}
