package Operator;

public class OperatorEx4 {

	public static void main(String[] args) {
		// 논리 연산자
		// &&(엠퍼샌드) : 두 항이 모두 참인 경우에만 결과값이 참(true) - 논리곱
		// || : bar 2개 : 두 항 중 하나만 참이어도 참(true) - 논리합
		System.out.println((4 < 5) && (10 == 11)); //false
		System.out.println((4 < 5) || (10 == 11)); //ture

	}

}
