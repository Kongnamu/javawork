package ifexample;

public class Seats {

	public static void main(String[] args) {
		// 입장객 수에 따른 좌석의 줄(행) 수 계산
		// 변수: 고객 수(customer), 좌석열 수(column), 줄 수(row)
		
		int customer = 23;	
		int column = 5;
		int row = 0;
		
		//연산(if ~ else문)
		row = customer / column;
		
		System.out.println(row + "줄이 필요합니다.");

	}

}
