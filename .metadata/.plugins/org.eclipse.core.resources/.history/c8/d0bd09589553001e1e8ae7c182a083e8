package student;

public class Student {
	private static Student instance;
	
	private Student() {}
	
	public static Student getInstance() {
		if(instance == null) {
			instance = new Student();
		}
		return instance;
	}

	//학번 생성 메서드
	public StudentNum creatStudentNum() {
		StudentNum studentNum = new studentNum();
		return studentNum;
	}
}
