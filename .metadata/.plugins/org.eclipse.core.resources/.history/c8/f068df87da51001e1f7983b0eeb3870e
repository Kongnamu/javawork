package overriding.shop; // 상속관계, 메서드 재정의 활용

public class MainClass {

	public static void main(String[] args) {
		Shop1 shop1 = new Shop1();
		
		shop1.된장찌개();
		shop1.김치찌개();
		shop1.비빔밥();
		
		System.out.println("=====================================");
		
		Shop2 shop2 = new Shop2();
		
		shop2.된장찌개();
		shop2.김치찌개();
		shop2.비빔밥();

	}

}
